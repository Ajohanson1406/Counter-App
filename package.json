{
  "name": "react-base",
  "version": "1.0.0",
  "description": "A basic react + babel + webpack project",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --open --mode development --port 3002",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
    "lint": "eslint src/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ajohanson1406/React-base.git"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Albert Johanson <alberttruk.11@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ajohanson1406/React-base/issues"
  },
  "homepage": "https://github.com/Ajohanson1406/React-base#readme",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "husky": "^4.3.0",
    "mini-css-extract-plugin": "^0.11.2",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
}
